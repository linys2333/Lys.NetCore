{
  "ConnectionStrings": {
    "Default": "Data Source=(LocalDb)\\MSSQLLocalDB;Initial Catalog=pro;Integrated Security=True"
  },
  "IdentityServer": {
    "ClinetId": "client",
    "ClinetSecret": "secret",
    "ApiName": "api1",
    "ApiSecret": "secret",
    "Authority": "http://localhost:5000/",
    "TokenUrl": "http://localhost:5000/connect/token",
    "RevocationUrl": "http://localhost:5000/connect/revocation"
  },
  "FileService": {
    "AppName": "AppName",
    "AppKey": "AppKey",
    "AppSecret": "AppSecret",
    "BaseUrl": "http://localhost:8888/"
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.RollingFile" ],
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "Async",
        "Args": {
          "configure": [
            {
              "Name": "RollingFile",
              "Args": {
                "pathFormat": "Logs/log-{Date}.txt",
                "outputTemplate": "[{Level}，{Timestamp:yyyy-MM-dd HH:mm:ss}] {Message}{NewLine}{Exception}",
                "shared": true,
                "fileSizeLimitBytes": null,
                "retainedFileCountLimit": null
              }
            }
          ]
        }
      }
    ]
  }
}